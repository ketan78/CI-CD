<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<listeners>
		<listener class-name="Selenium.TestComponents.Listeners"/>
	</listeners>
	
  <test thread-count="5" name="error">
    <classes>
      <class name="Selenium.Tests.error"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="end_to_end">
    <classes>
      <class name="Selenium.Tests.end_to_end"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
